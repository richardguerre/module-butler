{
  "name": "module-butler",
  "description": "A CLI that takes in module names and outputs a folder for each module name.",
  "version": "0.0.2",
  "author": "Richard Guerre @richardguerre",
  "bin": {
    "module-butler": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "build": "rm -rf tsconfig.tsbuildinfo && rm -rf lib && tsc -b",
    "prepack": "rm -rf tsconfig.tsbuildinfo && rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/richardguerre/module-butler/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "5.6.3",
    "global": "4.4.0",
    "handlebars": "^4.7.7",
    "log-symbols": "4.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^15.12.5",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "^2.3.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/richardguerre/module-butler",
  "keywords": [
    "react",
    "generator",
    "codegen",
    "component",
    "components",
    "module",
    "modules"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "module-butler",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "richardguerre/module-butler",
  "types": "lib/index.d.ts",
  "prettier": {
    "printWidth": 150,
    "singleQuote": true
  }
}
